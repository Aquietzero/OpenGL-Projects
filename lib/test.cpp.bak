#include <iostream>
#include "vector.h"

using namespace std;

int main() {

    Vector2D<double> v1(2, 9);
    Vector2D<double> v2(1, 2);
    Vector2D<double> v3, v4;

    v4 = v3 = v1 + v2;
    cout << v3.x << ", " << v3.y << endl;
    cout << v4.x << ", " << v4.y << endl;

    cout << "v4 * v3 = " << v4*v3 << endl;
    cout << "v4 cross v3 = " << v4.cross(v3) << endl;
    cout << "|v4| = " << v4.normal() << endl;

    v4.normalize();
    cout << "Normal(v4) = " << v4.x << ", " << v4.y << endl;

    Vector3D<double> n1(1, 2, 3);
    Vector3D<double> n2;

    n2 = Vector3D<double>(4, 5, 6);
    cout << "n2 = (" << n2.x << "," << n2.y << "," << n2.z << ")" << endl;

    return 0;

}
